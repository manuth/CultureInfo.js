{
  "name": "@manuth/culture-info",
  "version": "1.0.10",
  "description": "A node-package for representing locales",
  "author": "Manuel Thalmann <m@nuth.ch>",
  "license": "MIT",
  "keywords": [
    "culture-info",
    "culture",
    "language",
    "locale"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "homepage": "https://github.com/manuth/CultureInfo.js#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manuth/CultureInfo.js.git"
  },
  "bugs": {
    "url": "https://github.com/manuth/CultureInfo.js/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "rebuild": "npm run clean && npm run build",
    "watch": "npm run build -- --watch",
    "clean": "npm run build -- --clean && rimraf ./lib",
    "lint-base": "eslint --max-warnings 0 --ignore-pattern \"!.eslintrc.js\"",
    "lint": "npm run lint-base -- --ext .js,.jsx,.ts,.tsx ./src .eslintrc.js",
    "lint-ide": "npm run lint || exit 0",
    "test": "mocha",
    "prepare": "npm run rebuild"
  },
  "devDependencies": {
    "@manuth/eslint-plugin-typescript": "^2.3.9",
    "@manuth/tsconfig": "^1.2.9",
    "@manuth/tslint-presets": "^1.0.5",
    "@manuth/typescript-eslint-plugin": "^1.3.5",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.0.2",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "eslint-plugin-deprecation": "^1.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^32.3.0",
    "mocha": "^8.3.2",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4"
  }
}
